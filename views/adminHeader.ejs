<body>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <aside id="layout-menu" class="layout-menu menu-vertical menu bg-menu-theme">
                <div class="app-brand demo">
                    <a href="" class="app-brand-link">
                        <span class="app-brand-logo demo">
                            <span class="app-brand-text demo menu-text fw-bolder">
                                <img src="/img/logo.png" width="150px" height="60px" alt="">
                            </span>
                    </a>
                </div>

                <div class="menu-inner-shadow"></div>

                <ul class="menu-inner py-1">
                    <li class="menu-header small text-uppercase"><span class="menu-header-text">Manage Vendors</span></li>
                    <li class="menu-item open menu_toggler" data-filter="admin">
                        <a href="/admin" class="menu-link">
                            <i class="menu-icon tf-icons bx bx-detail"></i>
                            <div data-i18n="Form Layouts">Add Vendor</div>
                        </a>
                    </li>
                    <li class="menu-item open menu_toggler" data-filter="removeVendor">
                        <a href="/removeVendor" class="menu-link" data-filter="removeVendor">
